<p id="notice"><%= notice %></p>

<h1>Matches</h1>



<%= link_to rep5/cats2.jpeg(“cats2.jpg”),
 :class => ‘swipebox’ do %>
 <%= image_tag(‘cats2.jpeg’) %>
 <% end %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Age</th>
      <th>Sex</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matches.each do |match| %>
      <tr>
        <td><%= match.Title %></td>
        <td><%= match.Body %></td>
        <td><%= match.Age %></td>
        <td><%= match.Sex %></td>

        <td><%= link_to 'Show', match %></td>
        <td><%= link_to 'Edit', edit_match_path(match) %></td>
        <td><%= link_to 'Destroy', match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create Profile', new_match_path %>
